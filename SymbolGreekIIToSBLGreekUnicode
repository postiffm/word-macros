SymbolGreekIIMap(154) = " [A/" ' + smooth, acute, iota sub
SymbolGreekIIMap(155) = " {A/" ' + rough, acute, iota sub
SymbolGreekIIMap(156) = " \A/" ' + smooth, circum, iota sub
