SymbolGreekIIMap(121) = "w\/" ' + smooth, circum, iota sub
SymbolGreekIIMap(122) = "w|/" ' + rough, circum, iota sub
SymbolGreekIIMap(123) = "w;/" ' + grave, iota sub
SymbolGreekIIMap(124) = "w'/" ' + iota sub, circumflex
SymbolGreekIIMap(125) = "w/" ' + iota sub
SymbolGreekIIMap(126) = "wv/" ' + acute, iota sub
SymbolGreekIIMap(127) = "w'" ' + circumflex
SymbolGreekIIMap(128) = "w;" ' + grave
SymbolGreekIIMap(129) = "wv" ' + acute
SymbolGreekIIMap(130) = "wj" ' + smooth
SymbolGreekIIMap(131) = "wJ" ' + rough
SymbolGreekIIMap(132) = "w]" ' + smooth, grave
SymbolGreekIIMap(133) = "w}" ' + rough, grave
SymbolGreekIIMap(134) = "w[" ' + smooth, acute
SymbolGreekIIMap(135) = "w{" ' + rough, acute
SymbolGreekIIMap(136) = "w\" ' + smooth, circumflex
